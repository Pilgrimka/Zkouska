@model EvidencePojistenychWebAppASP.Models.Pojistenec

@{
    ViewData["Title"] = "Edit";
}



<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="mb-3">
                <label asp-for="Jmeno" class="fw-bold"></label>
                <input asp-for="Jmeno" class="form-control" />
                <span asp-validation-for="Jmeno" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label asp-for="Prijmeni" class="fw-bold"></label>
                <input asp-for="Prijmeni" class="form-control" />
                <span asp-validation-for="Prijmeni" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label asp-for="Email" class="fw-bold"></label>
                <input asp-for="Email" class="form-control" />
                <span asp-validation-for="Email" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label asp-for="TelCislo" class="fw-bold"></label>
                <input asp-for="TelCislo" class="form-control" />
                <span asp-validation-for="TelCislo" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label asp-for="UliceCisloPop" class="fw-bold"></label>
                <input asp-for="UliceCisloPop" class="form-control" />
                <span asp-validation-for="UliceCisloPop" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label asp-for="Mesto" class="fw-bold"></label>
                <input asp-for="Mesto" class="form-control" />
                <span asp-validation-for="Mesto" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label asp-for="Psc" class="fw-bold"></label>
                <input asp-for="Psc" class="form-control" />
                <span asp-validation-for="Psc" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <input type="submit" value="Uložit" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Zpět</a>
</div>

